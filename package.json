{
  "name": "@grandlinex/e-kernel",
  "version": "1.0.0",
  "description": "GrandLineX Electron Kernel",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "buildprep": "tsc",
    "test-run": "npm run buildprep && electron-forge start",
    "test-build": "npm run buildprep && electron-forge package ",
    "rebuild": "electron-rebuild",
    "lint": "eslint src",
    "makeDocs": "typedoc"
  },
  "keywords": [
    "typescript",
    "framework",
    "electron",
    "server",
    "backend"
  ],
  "author": {
    "name": "Elschnagoo"
  },
  "license": "BSD-3-Clause",
  "dependencies": {
    "@electron/rebuild": "3.6.0",
    "@grandlinex/core": "1.0.0",
    "dotenv": "16.4.5",
    "electron": "31.3.1",
    "node-abi": "3.65.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "res/img/favicon.png",
        "ignore": [
          ".idea",
          "scripts",
          "certs"
        ]
      }
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "7.4.0",
    "@types/node": "22.0.0",
    "@typescript-eslint/eslint-plugin": "7.18.0",
    "@typescript-eslint/parser": "7.18.0",
    "cross-env": "7.0.3",
    "electron": "31.3.1",
    "electron-devtools-installer": "3.2.0",
    "eslint": "8.57.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "18.0.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-jsx-a11y": "6.9.0",
    "eslint-plugin-prettier": "5.2.1",
    "html-webpack-plugin": "5.6.0",
    "prettier": "3.3.3",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
    "typedoc": "0.26.5",
    "typescript": "5.5.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GrandlineX/e-kernel.git"
  }
}
