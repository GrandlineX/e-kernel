{
  "name": "@grandlinex/e-kernel",
  "version": "0.30.3",
  "description": "GrandLineX Electron Kernel",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "buildprep": "tsc",
    "test-run": "npm run buildprep && electron-forge start",
    "test-build": "npm run buildprep && electron-forge package ",
    "rebuild": "electron-rebuild",
    "lint": "eslint src",
    "makeDocs": "typedoc"
  },
  "keywords": [
    "typescript",
    "framework",
    "electron",
    "server",
    "backend"
  ],
  "author": {
    "name": "Elschnagoo"
  },
  "license": "BSD-3-Clause",
  "dependencies": {
    "@electron/rebuild": "3.2.10",
    "@grandlinex/core": "0.30.1",
    "dotenv": "16.0.3",
    "electron": "23.2.1",
    "node-abi": "3.33.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "res/img/favicon.png",
        "ignore": [
          ".idea",
          "scripts",
          "certs"
        ]
      }
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "6.1.0",
    "@types/node": "18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "cross-env": "^7.0.3",
    "electron": "23.2.1",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^8.37.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "html-webpack-plugin": "^5.5.0",
    "prettier": "^2.8.7",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.28",
    "typescript": "^5.0.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GrandlineX/e-kernel.git"
  }
}
