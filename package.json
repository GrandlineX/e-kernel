{
  "name": "@grandlinex/e-kernel",
  "version": "0.25.0",
  "description": "GrandLineX Electron Kernel",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "buildprep": "tsc",
    "test-run": "npm run buildprep && electron-forge start",
    "test-build": "npm run buildprep && electron-forge package ",
    "rebuild": "electron-rebuild",
    "lint": "eslint src",
    "makeDocs": "typedoc"
  },
  "keywords": [
    "typescript",
    "framework",
    "electron",
    "server",
    "backend"
  ],
  "author": {
    "name": "Elschnagoo"
  },
  "license": "BSD-3-Clause",
  "dependencies": {
    "@grandlinex/core": "^0.25.0",
    "dotenv": "^10.0.0",
    "electron": "^18.0.0",
    "electron-rebuild": "^3.2.7",
    "node-abi": "^3.5.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "res/img/favicon.png",
        "ignore": [
          ".idea",
          "scripts",
          "certs"
        ]
      }
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@types/node": "^16.11.12",
    "@types/react": "^17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "cross-env": "^7.0.3",
    "electron": "^18.0.0",
    "electron-devtools-installer": "^3.2.0",
    "eslint": "^8.4.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "html-webpack-plugin": "^5.5.0",
    "prettier": "^2.5.1",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "typedoc": "^0.22.10",
    "typescript": "^4.5.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/GrandlineX/e-kernel.git"
  }
}
